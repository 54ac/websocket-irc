{
	"name": "websocket-irc",
	"description": "An IRC-like experience in the browser by using WebSockets.",
	"author": {
		"name": "rowrawer",
		"email": "rowrawer@gmail.com",
		"url": "https://github.com/rowrawer"
	},
	"repository": "github:rowrawer/websocket-irc",
	"version": "0.2.0",
	"private": true,
	"scripts": {
		"start": "npm run dev",
		"dev": "concurrently \"mongod --dbpath=db\" \"nodemon server.js\" \"webpack-dev-server --config dev.config.js --open\"",
		"back": "concurrently \"mongod --dbpath=db\" \"node server.js\"",
		"build": "webpack --config prod.config.js"
	},
	"devDependencies": {
		"@babel/core": "^7.10.5",
		"@babel/preset-env": "^7.10.4",
		"babel-loader": "^8.1.0",
		"concurrently": "^5.2.0",
		"css-loader": "^3.6.0",
		"html-loader": "^1.1.0",
		"html-webpack-plugin": "^4.3.0",
		"nodemon": "^2.0.4",
		"style-loader": "^1.2.1",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.12",
		"webpack-dev-server": "^3.11.0"
	},
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"mongodb": "^3.5.9",
		"ws": "^7.3.1"
	}
}
